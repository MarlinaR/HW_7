@model IEnumerable<task6_4.Book>
@{
    ViewBag.Title = "Ленивая загрузка дисков";
}

<table class="table">
    <tr><td>Книга</td><td>Автор</td><td>Страна</td></tr>
    @foreach (var item in Model)
    {
        <tr><td>@item.Name</td><td>@item.Author.Name</td><td>@item.Author.Country</td></tr>
    }
</table>